{
	"manifest_version": 2,
	"name": "Peeping Todd (Christmas Edition)",
	"description": "Special edition peeping Todd, just for you!",
	"version": "0.0.0.1",
	"permissions":
	[
		"tabs"
	],
	"content_scripts":
	[
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["peeping_todd.js"],
			"css": ["peeping_todd.css"]
		}
	],
	"web_accessible_resources":
	[
		"peeping_todd.png"
	]
}
