{
	"manifest_version": 2,
	"name": "Peeping Todd",
	"description": "Todd sure loves surprises, do you?",
	"version": "2.6.22.15",
	"permissions":
	[
		"tabs"
	],
	"content_scripts":
	[
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["peeping_todd.js"],
			"css": ["peeping_todd.css"]
		}
	],
	"web_accessible_resources":
	[
		"peeping_todd.png"
	]
}